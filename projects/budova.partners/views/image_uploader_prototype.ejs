<% disp = 'style=display:none' %>
  <title>Node File Uploads</title>
</head>
<body style="overflow:hidden">
<div id="imagedata">
    <%= typeof msg != 'undefined' ? msg : '' %>
  </div>

    <form id="media_uploader" action="/media_uploader" method="POST" enctype="multipart/form-data" <%= typeof file != 'undefined' ? disp : '' %>>
      <div class="file-field input-field">
        <div class="btn">
          <span><i class="fa fa-camera"></i></span>
          <input name="imageUpload" id="imageUpload" type="file">
        </div>
        <div class="file-path-wrapper">
          <input class="file-path validate" type="text">
        </div>
      </div>
    </form>

    <img src="<%= typeof file != 'undefined' ? file : '' %>" class="responsive-img">


  <script
  src="https://code.jquery.com/jquery-3.2.1.min.js"
  integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
  crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>

  <script>
$('.file-path-wrapper').on('change', 'input', function(){
let valo = jQuery(this).val()
$('body').attr('file_link', valo)

  $('form').css({"display": "none"})
  $('form').before('<div class="preloader loading"> <span class="slice"></span> <span class="slice"></span> <span class="slice"></span> <span class="slice"></span> <span class="slice"></span> <span class="slice"></span> </div>')
  setTimeout(function(){
    window.file_link = valo
    $('form').submit()
  }, 700)
})

function handlerFunction() {

}
let dropArea = document.getElementById('imageUpload')

  dropArea.addEventListener('dragenter', handlerFunction, false)



  </script>
</body>
